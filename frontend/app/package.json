{
  "name": "fleet",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "studio": "drizzle-kit studio",
    "introspect": "drizzle-kit introspect",
    "introspectsde": "drizzle-kit introspect --config=sde.drizzle.config.ts",
    "start": "astro dev",
    "check": "export $(cat .env.BUILDONLY) && astro check --minimumSeverity error",
    "build": "export $(cat .env.BUILDONLY) && astro check && astro build",
    "prodbuild": "export $(cat .env.BUILDONLY) && astro check --minimumSeverity error && astro build",
    "generate": "drizzle-kit generate --config=drizzle.config.ts",
    "push": "drizzle-kit push",
    "preview": "astro preview",
    "astro": "astro",
    "test": "export $(cat .env.BUILDONLY) && vitest",
    "coverage": "export $(cat .env.BUILDONLY) && vitest run --coverage",
    "test-ui": "export $(cat .env.BUILDONLY) && vitest --ui --coverage.enabled=true",
    "test-ci": "export $(cat .env.BUILDONLY) && vitest run --coverage --coverage.reporter=json --coverage.reportsDirectory=/artifacts/coverage --reporter=json --reporter=default --outputFile=/artifacts/front-end-unit"
  },
  "dependencies": {
    "@astrojs/check": "0.5.7",
    "@astrojs/node": "^9.2.2",
    "@astrojs/tailwind": "^5.1.5",
    "@fontsource-variable/montserrat": "^5.0.17",
    "@fontsource/norwester": "^5.0.7",
    "@fontsource/qahiri": "^5.0.8",
    "@justinribeiro/lite-youtube": "^1.5.0",
    "@midudev/tailwind-animations": "^0.0.7",
    "@sentry/astro": "^7.110.0",
    "@toast-ui/editor": "^3.1.0",
    "@toast-ui/editor-plugin-code-syntax-highlight": "^3.1.0",
    "@types/alpinejs": "^3.13.5",
    "@types/marked": "^6.0.0",
    "astro": "^5.7.14",
    "astro-tooltips": "^0.6.2",
    "better-sqlite3": "^11.10.0",
    "cheerio": "^1.0.0-rc.12",
    "countdown": "^2.6.0",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "0.43.1",
    "flatpickr": "^4.6.13",
    "highlight.js": "^11.9.0",
    "jose": "^5.2.0",
    "marked": "^11.1.0",
    "marked-highlight": "^2.1.1",
    "moment": "^2.30.1",
    "slugify": "^1.6.6",
    "tailwindcss": "^3.3.6",
    "tippy.js": "^6.3.7",
    "typescript": "^5.3.2",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@types/gtag.js": "^0.0.20",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "happy-dom": "^17.4.7",
    "sass": "^1.69.5",
    "vitest": "^3.1.4"
  }
}
