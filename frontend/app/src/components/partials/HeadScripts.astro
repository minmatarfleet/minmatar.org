<script>
	import { navigate } from 'astro:transitions/client'

	(window as any).navigate = function (route) {
		return navigate(route)
	}
</script>

<script>
	import _slugify from 'slugify'

	(window as any).slugify = function (string) {
		return _slugify(string, {
			replacement: '_',
			remove: /[*+~.,\-\()'"!:@]/g
		}).toLowerCase()
	}
</script>

